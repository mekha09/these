@Article{ParienteOL2015,
  Title                    = {Spatio-temporal light springs: extended encoding of orbital angular momentum in ultrashort pulses},
  Author                   = {G. Pariente and F. Qu\'{e}r\'{e}},
  Journal                  = {Opt. Lett.},
  Year                     = {2015},
  Month                    = {May},
  Number                   = {9},
  Pages                    = {2037--2040},
  Volume                   = {40},
  Abstract                 = {We introduce a new class of spatio-temporally coupled ultrashort laser beams, which are obtained by superimposing Laguerre\&\#x2013;Gauss beams whose azimuthal mode index is correlated to their frequency. These beams are characterized by helical structures for their phase and intensity profiles, which both encode the orbital angular momentum carried by the light. They can easily be engineered in the optical range, and are naturally produced at shorter wavelengths when attosecond pulses are generated by intense femtosecond Laguerre\&\#x2013;Gauss laser beams. These spatio-temporal \&\#x201C;light springs\&\#x201D; will allow for the transfer of the orbital angular momentum to matter by stimulated Raman scattering.},
  DOI                      = {10.1364/OL.40.002037},
  Keywords                 = {Ultrafast optics; Pulse shaping; Optical vortices ; Ultrafast phenomena},
  Publisher                = {OSA},
